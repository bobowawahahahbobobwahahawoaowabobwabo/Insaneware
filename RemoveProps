wait()
	for i,v in pairs(game.Workspace.Explosions:GetChildren()) do 
					if v.Name == "BombProp" then 
						for a,b in pairs(v:GetChildren()) do
							b:Destroy() 
				end end end
	for i,v in pairs(game.Workspace.Explosions:GetChildren()) do 
					if v.Name == "Molotovprop" then 
						for a,b in pairs(v:GetChildren()) do
							b:Destroy() 
				end end end 
wait()
